{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "MySql:CONNECTIONSTRING": "Server=localhost;Port=3306;Database=test;Uid=root;Pwd=example;",
    "Management": {
        "Port": 8081,
        "Endpoints": {
            "Path": "",
            "Actuator": {
                "Exposure": {
                    "Include": [ "*" ]
                },
                "SerializerOptions": {
                    "WriteIndented": true
                },
                "CustomJsonConverters": [
                    "Steeltoe.Management.Endpoint.Actuators.Info.EpochSecondsDateTimeConverter"
                ]
            }
        }
    }
    //server=localhost:3306;uid=root;pwd=example;database=test"
}
